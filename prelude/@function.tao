: (a -> b, b -> c) -> (a -> c)
let f >> g = x -> g(f(x))

: (b -> c, a -> b) -> (a -> c)
let g << f = f >> g

> (x -> x + x >> x -> x * x)(3) ~> 36
> (x -> x + x >> x -> x * x)(3.0) ~> 36.0
> (x -> x + x << x -> x * x)(3) ~> 18
> (x -> x + x << x -> x * x)(3.0) ~> 18.0

: (a, a -> b) -> b
let x |> f = f(x)

: (a -> b, a) -> b
let f <| x = x |> f

> 3 |> (x -> x + x) ~> 6
> 3.0 |> (x -> x + x) ~> 6.0
> (x -> x + x) <| 3 ~> 6
> (x -> x + x) <| 3.0 ~> 6.0
