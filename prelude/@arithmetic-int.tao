: (Int, Int) -> Int
let x + y = %int_add(x, y)

> 3 + 2 ~> 5

: (Int, Int) -> Int
let x - y = %int_sub(x, y)

> 3 - 2 ~> 1

: (Int, Int) -> Int
let x * y = %int_mul(x, y)

> 3 * 2 ~> 6

: (Int, Int) -> Num
let x / y = %int_div(x, y)

> 3 / 2 ~> 1.5

: (Int, Int) -> Int
let x ^ y = %int_pow(x, y)

> 3 ^ 2 ~> 9
