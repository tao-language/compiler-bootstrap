: (Int, Int) -> Bool
x < y = %int_lt(x, y)

: (Num, Num) -> Bool
x < y = %num_lt(x, y)

: (a, a) -> Bool
(==) = match {
| (x, x) -> True
| _ -> False
}

: (a, a) -> Bool
x != y = not (x == y)

: (a, a) -> Bool
x <= y = not (y < x)

: (a, a) -> Bool
x > y = y < x

: (a, a) -> Bool
x >= y = not (x < y)
